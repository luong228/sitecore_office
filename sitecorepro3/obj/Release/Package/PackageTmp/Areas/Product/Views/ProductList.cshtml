@using Sitecore.Data.Fields
@using Sitecore.Data.Items
@model RenderingModel
@{
    ViewBag.Title = "Product";
}
@{
    Item currentItem = RenderingContext.Current.Rendering.Item;
    MultilistField treelistField = (MultilistField)currentItem.Fields["productList"];
    Item[] selectedItems = treelistField.GetItems();
}
<div class="container">
    <div class="row productList"></div>
    @foreach (var item in selectedItems)
    {
        <div class="product">
            <h3>@Html.Sitecore().Field("Name", item)</h3>
            <p>@Html.Sitecore().Field("Description", item)</p>
            <p>Price: @Html.Sitecore().Field("Price", item)</p>
            <p>Quantity: @Html.Sitecore().Field("Quantity", item)</p>
        </div>
    }
</div>